# Flutter Environment Configuration
# Copy this file to .env and update the values with your Stream credentials

# Stream Chat API Key (from your Stream Dashboard)
STREAM_CHAT_API_KEY=your_stream_chat_api_key_here

# Stream Chat API Secret (from your Stream Dashboard)
# Keep this secret and never commit it to version control
STREAM_CHAT_API_SECRET=your_stream_chat_secret_here

# Base URL for your token server (for production, replace with your server URL)
TOKEN_SERVER_URL=http://localhost:3000

# App Environment (development, staging, production)
APP_ENV=development

# Debug mode (true/false)
DEBUG=true
